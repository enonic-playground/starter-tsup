  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enonic/starter-tsup"
  },
  "scripts": {
    "clean": "rimraf build",
    "prewatch:browser": "npm run tsup:browser",
    "prewatch:server": "npm run tsup:server",
    "tsup": "npm-run-all --parallel tsup:browser tsup:server",
    "tsup:browser": "npx tsup -d build/resources/main/assets",
    "tsup:server": "npx tsup -d build/resources/main",
    "watch": "npm-run-all --parallel watch:browser watch:server",
    "watch:browser": "npx tsup -d build/resources/main/assets --watch src/main/resources/assets",
    "watch:server": "npx tsup -d build/resources/main --watch src/main/resources --ignore-watch src/main/resources/assets"
  }
}
